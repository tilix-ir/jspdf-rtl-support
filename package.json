{
"name": "jspdf-rtl-support",
"version": "0.2.7",
"description": "RTL rich text printing utilities for jsPDF with justification and styling.",
"type": "module",
"main": "dist/rtl-support.js",
"module": "dist/rtl-support.js",
"types": "dist/rtl-support.d.ts",
"exports": {
".": {
"types": "./dist/rtl-support.d.ts",
"import": "./dist/rtl-support.js",
"require": "./dist/rtl-support.cjs"
}
},
"files": [
"dist",
"README.md",
"CHANGELOG.md"
],
"keywords": [
"jsPDF",
"RTL",
"Persian",
"Arabic",
"text",
"printer",
"justify"
],
"peerDependencies": {
"jspdf": ">=2.5.1"
},
"devDependencies": {
"typescript": "^5.6.3"
},
"scripts": {
"build": "tsc -p tsconfig.json",
"build:cjs": "node scripts/build-cjs.mjs",
"typecheck": "tsc -p tsconfig.json --noEmit",
"test:render": "node scripts/test-render.mjs",
"prepublishOnly": "npm run build && npm run build:cjs && npm run typecheck"
},
"author": "tilix-coders [contact@tilix.ir](mailto:contact@tilix.ir)",
"license": "MIT",
 "repository": {
    "type": "git",
    "url": "git+https://github.com/tilix-ir/jspdf-rtl-support.git"
  }
}
